{
	"compilerOptions": {
		"module": "commonjs",
		"declaration": false,
		"removeComments": true,
		"allowSyntheticDefaultImports": true,
		"target": "ES2021",
		"incremental": true,
		"skipLibCheck": true,
		"strictNullChecks": false,
		"noImplicitAny": true,
		"strictBindCallApply": false,
		"forceConsistentCasingInFileNames": false
	}
}
