{
	"compilerOptions": {
		"moduleResolution": "Node",
		"target": "ESNext",
		"module": "ESNext",
		"declaration": false,
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": false,
		"forceConsistentCasingInFileNames": true,
		"strictNullChecks": true,
		"strictBindCallApply": false,
		"baseUrl": ".",
		"paths": {
			"tslib": ["./node_modules/tslib/tslib.d.ts"],
			"~/*": ["./src/*"]
		}
	}
}
